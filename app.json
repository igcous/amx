{
  "expo": {
    "name": "amx",
    "slug": "amx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/amx_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/amx_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.igcous.amx",
      "googleServicesFile": "./config/GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/amx_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.igcous.amx",
      "googleServicesFile": "./config/google-services.json"
    },
    "web": {
      "favicon": "./assets/amx_icon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "./plugins/notifee-mod.js"
    ],
    "scheme": "my-app-scheme"
  }
}